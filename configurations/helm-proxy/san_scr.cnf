[ req ]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn

[ dn ]
C = US
ST = California
L = San Francisco
O = My Company
OU = My Division
emailAddress = my@email.com
CN = httpd-proxy.default:80

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = httpd-proxy.default:80
DNS.2 = httpd-proxy
